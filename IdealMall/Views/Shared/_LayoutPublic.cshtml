                    <div id="total"><span class="text">Total</span> <span id="shoppingtotal" class="sum">£@Math.Round(totalamount, 2)</span></div>
                }
                <div class="adrightdiv">
                    <p>Advertisement Here</p>
                </div>

            </div>

